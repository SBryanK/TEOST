    <!-- Help & Privacy -->
    <string name="help_body">Use this guide to run tests, read results, and manage settings.</string>
    <string name="help_quick_start">Quick Start</string>
    <string name="help_quick_start_points">Open Tests → Pick a category → Configure → Add to Cart → Confirm &amp; Start → Review Success screen or History.</string>
    <string name="help_navigation">App Navigation</string>
    <string name="help_nav_points">Tests: browse &amp; configure; Search: quick checks; History: past results; Profile: language &amp; info.</string>
    <string name="help_tests_screen">Tests Screen</string>
    <string name="help_tests_points">Import/Export configs under the centered title. Tap a category card to proceed. Uniform cards, colored icons, centered text.</string>
    <string name="help_configure">Configure Test</string>
    <string name="help_configure_points">Enter target and adjust parameters. Use dropdowns for options and sliders where applicable. Add to Cart when valid.</string>
    <string name="help_flow">Confirmation → Execution → Success</string>
    <string name="help_flow_points">Confirm details, then see a loading overlay. Results show as cards; go to History or back to Tests.</string>
    <string name="help_search">Search Screen</string>
    <string name="help_search_points">Quick connection test for a single target. Two-line metric chips. Last results cached for 1 hour.</string>
    <string name="help_results">Reading Results</string>
    <string name="help_results_points">Cards summarize status and metrics (DNS, TCP, TTFB, SSL). Use Show more for details and logs.</string>
    <string name="help_import_export">Import/Export</string>
    <string name="help_import_export_points">Import JSON to prefill, or Share current config. You’ll see feedback on success or invalid JSON.</string>
    <string name="help_profile_language">Profile &amp; Language</string>
    <string name="help_profile_language_points">Toggle EN/中文 instantly. Safe area paddings ensure content isn’t obscured by the navbar.</string>
    <string name="help_faq_title">FAQ</string>
    <string name="help_faq_q1">Can I run multiple tests at once?</string>
    <string name="help_faq_a1">Yes. Add multiple configurations to the cart and confirm.</string>
    <string name="help_faq_q2">Where do I see finished tests?</string>
    <string name="help_faq_a2">On the Success screen (inline cards) and in History.</string>
    <string name="help_faq_q3">Do I need internet to switch languages?</string>
    <string name="help_faq_a3">No. Language changes apply instantly within the app.</string>
    <string name="help_faq_q4">What format does Import support?</string>
    <string name="help_faq_a4">JSON using the app’s configuration schema.</string>
    <string name="help_faq_q5">Why did my Search results disappear?</string>
    <string name="help_faq_a5">Search results are cached for 1 hour. After that, rerun the test.</string>
    <string name="help_faq_q6">How do I read DNS/TCP/TTFB/SSL metrics?</string>
    <string name="help_faq_a6">They reflect resolution, connection, first byte, and TLS timing respectively. Lower is generally better.</string>
    <string name="help_faq_q7">How do I report an issue?</string>
    <string name="help_faq_a7">Use your standard support channel and include screenshots or exported configs if relevant.</string>
    <string name="privacy_title">Privacy Policy</string>
    <string name="privacy_body">This is a mock privacy policy. Use this app only on authorized domains. Test results may be cached locally (Room) and preferences stored (DataStore). Avoid sharing PII. Review and adjust your usage according to your organization’s policies.</string>
